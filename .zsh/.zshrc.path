# PATH

# brew
export PATH=/usr/local/bin:$PATH

# anyenv
export PATH=~/.anyenv/bin:$PATH
eval "$(anyenv init -)"

# rbenv
export PATH="$HOME/.rbenv/bin:$PATH"
eval "$(rbenv init -)"

